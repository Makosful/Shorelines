<AnchorPane id="AnchorPane" prefHeight="468.0" prefWidth="778.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.makosful.shoreline.gui.controller.TaskManagerWindowController">
   <children>
      <VBox layoutX="67.0" layoutY="35.0" prefHeight="367.0" prefWidth="294.0">
         <children>
            <ListView fx:id="taskListView" prefHeight="367.0" prefWidth="282.0" />
         </children>
      </VBox>
      <VBox layoutX="396.0" layoutY="35.0" prefHeight="367.0" prefWidth="294.0">
         <children>
            <ListView fx:id="runningListView" prefHeight="595.0" prefWidth="257.0" />
         </children>
      </VBox>
